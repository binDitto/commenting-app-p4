<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-sm-offset-3">
      <div class="panel panel-default">
        <div class="panel-heading">
          <div class="media-left">
            <div class="btn-group">
              <%= link_to 'Back', @post, class: "btn btn-default" %>
            </div>
          </div>
          <h1>Editing Post</h1>
        </div>
        <div class="panel-body">
          <%= render 'form', post: @post %>
        </div>
      </div>
    </div>
  </div>
</div>
